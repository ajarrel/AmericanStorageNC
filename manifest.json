{
	"background":{
		"scripts": ["background.js"]
	},
	
	"manifest_version": 2,
	"name": "American Self Storage Helper",
	"version": "0.103",
	
	"description": "A helpful tool for American Self Storage employees",
	
	"permissions": [
		"alarms",
		"background",
		"clipboardRead",
		"clipboardWrite",
		"desktopCapture",
		"idle",
		"notifications",
		"storage",
		"http://*/*",
		"https://*/*",
		"tabs"
	],
	
	"browser_action": {
		"default_title": "Open Rates Page",
		"default_popup": "popup.html"
	}
	
}